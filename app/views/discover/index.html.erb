<h1> Discover Movies </h1>

<%= button_to "Discover Top Rated Movies", user_movies_path(@user), method: :get, params:{discover: :top_rated} %>

<%= form_with url: user_movies_path(@user), method: :get do |form|%>
  <%= form.search_field :keyword, required: true %>
  <%= form.submit "Search by Movie Title"%>
<% end %>